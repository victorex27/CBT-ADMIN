                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ChoiceBox>
                        <Hyperlink fx:id="assignmentLink" layoutX="148.0" layoutY="85.0" text="Set up Assignment without file upload" visible="false">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Hyperlink>
                        <Pane fx:id="datePane" layoutX="373.0" layoutY="35.0">
                           <children>
                              <TextField fx:id="hourTextField" layoutX="1.0" layoutY="38.0" prefHeight="25.0" prefWidth="49.0" text="00" />
                              <Label layoutX="59.0" layoutY="42.0" text="h" textFill="WHITE">
