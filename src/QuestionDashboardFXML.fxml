                              <TextField fx:id="minuteTextField" layoutX="79.0" layoutY="38.0" prefHeight="25.0" prefWidth="49.0" text="00" />
                           </children>
                        </Pane>
                     </children>
                  </Pane>
               </children>
            </StackPane>
            <Button fx:id="saveToDbButtuon" layoutX="627.0" layoutY="301.0" mnemonicParsing="false" onAction="#saveToDB" prefHeight="56.0" prefWidth="99.0" styleClass="whiteButton" text="Save" textFill="#f2f2f2">
               <font>
                  <Font size="17.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </Pane>
    </children>
</AnchorPane>
