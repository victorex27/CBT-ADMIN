<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1024.0" stylesheets="@loginfxml.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginFXMLDocumentController">
    <children>
        <fx:include source="FrameFXML.fxml" />
      <Pane layoutX="275.0" layoutY="283.0" prefHeight="262.0" prefWidth="475.0">
         <children>
            <TextField fx:id="username" layoutX="65.0" layoutY="50.0" prefHeight="49.0" prefWidth="375.0" promptText="Username" styleClass="textField">
               <font>
                  <Font size="20.0" />
               </font></TextField>
            <PasswordField fx:id="password" layoutX="65.0" layoutY="122.0" prefHeight="49.0" prefWidth="375.0" promptText="Password" styleClass="passwordField">
               <font>
                  <Font size="17.0" />
               </font></PasswordField>
            <Button fx:id="button" layoutX="348.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleLoginAction" prefHeight="43.0" prefWidth="87.0" text="Login" textFill="#ebe7e7">
               <font>
                  <Font size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <effect>
                  <DropShadow />
               </effect></Button>
         </children>
      </Pane>
      <Label fx:id="warningText" layoutX="388.0" layoutY="580.0" maxWidth="1.7976931348623157E308" opacity="0.66" prefHeight="34.0" text="Label" textFill="#f8f7f7" visible="false">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <ImageView fx:id="warningImage" fitHeight="29.0" fitWidth="55.0" layoutX="346.0" layoutY="582.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@image/warning.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
